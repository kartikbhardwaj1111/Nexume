# Nexume Environment Configuration

# Google Gemini AI API Key (Optional - fallback analysis available)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Job Search APIs (Optional - fallback jobs available)
VITE_ADZUNA_APP_ID=your_adzuna_app_id
VITE_ADZUNA_APP_KEY=your_adzuna_app_key

# Application Settings
VITE_APP_NAME=Nexume
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Analytics (Optional)
VITE_ANALYTICS_ID=your_analytics_id

# Feature Flags
VITE_ENABLE_COMPREHENSIVE_ANALYSIS=true
VITE_ENABLE_JOB_MATCHING=true
VITE_ENABLE_TEMPLATE_GENERATION=true
VITE_ENABLE_CAREER_GUIDANCE=true

# Security
VITE_ENABLE_FILE_ENCRYPTION=false
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,docx,txt

# Performance
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_CODE_SPLITTING=true
VITE_CACHE_DURATION=3600000